{
    "name":        "wakeapp/dbal-enum-type-bundle",
    "description": "Provides functionality for working with ENUM type in the Doctrine",
    "type":        "symfony-bundle",
    "keywords":    ["dbal", "enum", "dbal-type", "doctrine", "doctrine-enum-type", "symfony-bundle"],
    "license":     "MIT",

    "require": {
        "php":                      "~7.1",
        "ext-mbstring":             "*",
        "doctrine/dbal":            "~2.0",
        "doctrine/doctrine-bundle": "^1.6",
        "wakeapp/dbal-enum-type":   "^1.0"
    },

    "conflict": {
        "wakeapp/enumer-bundle": "<1.0"
    },

    "suggest": {
        "wakeapp/enumer-bundle": "Provides possibility to avoid reflection on the runtime"
    },

    "autoload": {
        "psr-4": {
            "Wakeapp\\Bundle\\DbalEnumTypeBundle\\": ""
        }
    }
}
